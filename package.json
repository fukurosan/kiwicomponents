{
  "name": "kiwicomponents",
  "version": "0.4.0",
  "description": "An SDK for web browsers.",
  "main": "./dist/js/bundle.js",
  "unpkg": "./dist/js/bundle.js",
  "jsdelivr": "./dist/js/bundle.js",
  "module": "./dist/js/esm-bundle.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "lint:fix": "prettier \"src/**/*.js\" --write && eslint \"src/**/*.js\" --fix",
    "test": "jest --no-cache --verbose",
    "build": "npm run lint:fix && rimraf dist && npm run build:rollup && npm run test && npm run build:types",
    "build:rollup": "npx rollup -c",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir ./dist/types/",
    "start": "npx rollup -c rollup.config.dev.js --watch"
  },
  "author": "Henrik Olofsson",
  "license": "MIT",
  "keywords": [
    "browser-sdk",
    "web-components",
    "window-management",
    "window",
    "toast",
    "notifications",
    "tooltip",
    "context-menu",
    "kiwi",
    "kiwi-components",
    "web-sdk"
  ],
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.4.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-image-snapshot": "^4.3.0",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "puppeteer": "^11.0.0",
    "rimraf": "^3.0.1",
    "rollup": "^2.38.4",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.54.9",
    "typescript": "^4.5.5"
  }
}
